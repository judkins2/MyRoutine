@model MyRoutine.Models.toggleRoutineItemViewModel
<script>
        function toggleRoutineItem(id)
        {
            $.ajax('@Url.Action("toggle","Dashboard")', {
                type: 'POST',
                contentType: "application/json",
                dataType: "json",
                traditional: true,
                success: function (returnData) {
                    $('#AB').html(returnData.ViewString);
                    loadModels(returnData.ModelList);
                },
                error: function () {
                }
            });
        }

</script>


@using (Html.BeginForm())
{
    foreach (var item in Model.RoutineList)
    {
        <div>
            <input type="submit" value="@item.RoutineName" onclick="toggleRoutineItem(@item.Id)" class="form-control" />
            <br /><br />
        </div>
    }
}


